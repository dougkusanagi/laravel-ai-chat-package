{
    "name": "dougkusanagi/laravel-ai-chat",
    "description": "A Laravel AI Chat package with database integration",
    "type": "library",
    "version": "0.1.20",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "homepage": "https://github.com/dougkusanagi/laravel-ai-chat",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0",
        "google-gemini-php/client": "^1.0",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Dougkusanagi\\LaravelAiChat\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Douglas Lopes",
            "email": "dl.aguiar@yahoo.com.br"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Dougkusanagi\\LaravelAiChat\\AiChatServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
